# Порт, на котором запускается сервер
SERVER_PORT=8080

# Режим работы (development/production)
ENV=development

# Таймауты (в секундах или миллисекундах)
SERVER_READ_TIMEOUT=30s      # Таймаут на чтение запроса
SERVER_WRITE_TIMEOUT=30s     # Таймаут на запись ответа
SERVER_IDLE_TIMEOUT=60s      # Таймаут бездействия подключения

# Настройки Codewars API
CODEWARS_API_URL=https://www.codewars.com/api/v1

# Настройки базы данных
DB_DSN=postgres://solverapi:12345678@localhost:5432/codewars?sslmode=disable&connect_timeout=10